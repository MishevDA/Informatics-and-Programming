#include <math.h>
#include <stdio.h>
#include <locale.h>
int main()
{
	setlocale(LC_ALL, "Russian");

	double pdsp = 600; //ďëîňíîńňü äńď
	double pdvp = 800; // ďëîňíîńňü äâď
	double pder = 1000; // ďëîňíîńňü äĺđĺâŕ
	double h, w, t1 = 0.005, t2 = 0.015, t3 = 0.01, hui, d, V1,V2,V3;
	
	printf("ââĺäčňĺ âűńîňó řęŕôŕ(îň 180 äî 220):\n");
	scanf_s("%lf", &h);
	
	printf("ââĺäčňĺ řčđčíó řęŕôŕ(îň 80 äî 120):\n");
	scanf_s("%lf", &w);

	printf("ââĺäčňĺ ăëóáčíó řęŕôŕ(îň 50 äî 90):\n"); 
	scanf_s("%lf", &d);
	if (h < 200) {
		h = h / 100;
		w = w / 100;
		d = d / 100;
		V1 = (h * w * t1) ;
		V2 =((h * d * t2) * 2) + ((w * d * t2) * 2) + ((d * w * t2) * 4);//â çŕäŕíčč íĺ óęŕçŕíŕ ňîëůčíŕ ďîëîę,˙ âç˙ë 15 ěčëëčěĺňđîâ
		V3 = (h * w * t3);
		hui = (V1 * pdvp) + (V2 * pdsp) + (V3 * pder);
		printf("%lf", &hui);
	}
	else if (h >= 200) {
		h = h / 100;
		w = w / 100;
		d = d / 100;
		V1 = (h * w * t1) ;
		V2 = ((h * d * t2) * 2) + ((w * d * t2) * 2) + ((d * w * t2) * 5);
		V3 = (h * w * t3);
		hui = (V1 * pdvp) + (V2 * pdsp) + (V3 * pder);
		printf("%lf", &hui);
	}
	return 0;
}
